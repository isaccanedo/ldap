package com.ldap.autenticacao.constante;

public enum KeyType {

	ZMK("00"), ZPK("01"), TMK("02"), TPK("02"), ZEK("0A"), TEK("0B"), IDP("99");

	private String keyTypeCode;

	private KeyType(final String keyTypeCode) {
		this.keyTypeCode = keyTypeCode;

	}

	public String value() {
		return this.keyTypeCode;
	}

	public static KeyType fromKeyTypeName(final String keyTypeName) {
		for (final KeyType rc : KeyType.values()) {
			if (rc.name().equalsIgnoreCase(keyTypeName)) {
				return rc;
			}
		}

		return null;
	}

	@Override
	public String toString() {

		return this.name();
	}

}
